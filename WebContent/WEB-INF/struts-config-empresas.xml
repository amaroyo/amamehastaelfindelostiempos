<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>	
	
	<form-beans>
		<form-bean name="GridEmpresasForm" type="es.oyssen.mrm.struts.forms.empresas.GridEmpresasForm" />
		<form-bean name="EditarEmpresaForm" type="es.oyssen.mrm.struts.forms.empresas.EmpresaForm" />		
	</form-beans>
	

	<action-mappings>
		<action path="/empresas/inicio" forward="/jsp/web/empresas/inicio.jsp" />
		
		<action path="/empresas/gridempresas" name="GridEmpresasForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.empresas.GridEmpresasAction">

		</action>
		
		<action path="/empresas/editarempresa" name="EditarEmpresaForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.empresas.EditarEmpresaAction"  />

		<action path="/empresas/gridleads" name="GridEmpresasForm" scope="request"  type="es.oyssen.mrm.struts.actions.empresas.GridLeadsAction" />
				
		<action path="/empresas/actualizarempresa" name="EditarEmpresaForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.empresas.EditarEmpresaAction"  />
		
		<action path="/empresas/exportarempresas" name="GridEmpresasForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.empresas.ExportarEmpresasAction" />
		
		<action path="/empresas/cargarempresa" name="EditarEmpresaForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.empresas.CargarEmpresaAction"  />
		
	</action-mappings>

</struts-config>