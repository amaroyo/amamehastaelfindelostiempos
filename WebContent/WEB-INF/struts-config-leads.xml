<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>	
	
	<form-beans>
		<form-bean name="ImportarLeadsForm" type="es.oyssen.mrm.struts.forms.leads.ImportarLeadsForm" />
		<form-bean name="GridLeadsForm" type="es.oyssen.mrm.struts.forms.leads.GridLeadsForm" />
		<form-bean name="EditarLeadForm" type="es.oyssen.mrm.struts.forms.leads.LeadForm" />
		<form-bean name="ActionsHistoryForm" type="es.oyssen.mrm.struts.forms.leads.ActionsHistoryForm" />
		<form-bean name="GridActionsHistoryForm" type="es.oyssen.mrm.struts.forms.leads.GridActionsHistoryForm" />
		<form-bean name="NuevoLeadForm" type="es.oyssen.mrm.struts.forms.leads.NuevoLeadForm" />
		
		<form-bean name="GridFicherosForm" type="es.oyssen.mrm.struts.forms.ficheros.GridFicherosForm" />
		<form-bean name="EditarFicheroForm" type="es.oyssen.mrm.struts.forms.ficheros.FicheroForm" />		
		<form-bean name="SubirFicheroForm" type="es.oyssen.mrm.struts.forms.ficheros.SubirFicheroForm" />		
	</form-beans>
	

	<action-mappings>
		<action path="/leads/inicio" forward="/jsp/web/leads/inicio.jsp" />
		
		<action path="/leads/importar" forward="/jsp/web/leads/importar.jsp" />
		
		<action path="/leads/importarLeads" name="ImportarLeadsForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.leads.ImportarLeadsAction">
			<exception type="java.lang.Exception" key="error.importar.leads" path="/leads/importar.do"></exception>
			<forward name="success" path="/leads/importar.do" />
		</action>

		<action path="/leads/gridleads" name="GridLeadsForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.leads.GridLeadsAction">

		</action>

		<action path="/leads/editarlead" name="EditarLeadForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.leads.EditarLeadAction"  />
		
		<action path="/leads/actualizarlead" name="EditarLeadForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.leads.EditarLeadAction"  />

		<action path="/leads/nuevolead" name="NuevoLeadForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.leads.NuevoLeadAction"  />
		
		<action path="/leads/actionshistory" name="ActionsHistoryForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.leads.ActionsHistoryAction"  />
		
		<action path="/leads/gridactionshistory" name="GridActionsHistoryForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.leads.GridActionsHistoryAction" />
		
		
		<action path="/leads/listarcanales" name="" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.canales.ListarCanalesAction"  />
		<action path="/leads/listarservicios" name="" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.servicios.ListarServiciosAction"  />
		<action path="/leads/listardistribuidores" name="" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.distribuidores.ListarDistribuidoresAction"  />
		<action path="/leads/listarempresas" name="" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.empresas.ListarEmpresasAction"  />
		<action path="/leads/listarresponsables" name="" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.responsables.ListarResponsablesAction"  />
		<action path="/leads/listarcomerciales" name="" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.comerciales.ListarComercialesAction"  />

		<action path="/leads/exportarleads" name="GridLeadsForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.leads.ExportarLeadsAction" />
		
		<action path="/leads/gridleadshistory" name="GridLeadsForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.leads.GridLeadsHistoryAction" />




		<action path="/leads/gridficheros" name="GridFicherosForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.leads.GridFicherosAction" />

		<action path="/leads/editarfichero" name="EditarFicheroForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.leads.EditarFicheroAction"  />
		
		<action path="/leads/actualizarfichero" name="EditarFicheroForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.leads.EditarFicheroAction"  />

		<action path="/leads/subir" forward="/jsp/web/leads/subir.jsp" />		
		
		<action path="/leads/descargar" name="DescargaFichero" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.leads.DescargarFicheroAction" />

		<action path="/leads/subirFichero" name="SubirFicheroForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.leads.SubirFicheroAction">
			<exception type="java.lang.Exception" key="error.subir.fichero" path="/leads/subir.do"></exception>
			<forward name="success" path="/leads/subir.do" />
		</action>
		
	</action-mappings>

</struts-config>