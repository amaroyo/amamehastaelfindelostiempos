<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://struts.apache.org/dtds/struts-config_1_3.dtd">


<struts-config>	
	
	<form-beans>
	    
		<form-bean name="GridUsuarios" type="es.oyssen.mrm.struts.forms.usuarios.GridUsuariosForm" />
	   
	   	<form-bean name="EditarUsuarioForm" type="es.oyssen.mrm.struts.forms.usuarios.EditarUsuarioForm" />
		
	</form-beans>
	

	<action-mappings>

		<action path="/curso_academico/inicio" forward="/jsp/web/curso_academico/inicio.jsp" />
		
		<action path="/curso_academico/gridusuarios" name="GridUsuarios" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.usuarios.GridUsuariosAction" />
		
		<action path="/curso_academico/editarusuario" name="EditarUsuarioForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.usuarios.EditarUsuarioAction"  />
		
		<action path="/curso_academico/actualizarusuario" name="EditarUsuarioForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.usuarios.EditarUsuarioAction"  />
		
	</action-mappings>

</struts-config>