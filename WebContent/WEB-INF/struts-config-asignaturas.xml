<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://struts.apache.org/dtds/struts-config_1_3.dtd">


<struts-config>	
	
	<form-beans>
	    
	   <form-bean name="GridUsuarios" type="es.oyssen.mrm.struts.forms.usuarios.GridUsuariosForm" />
	   
	   <form-bean name="EditarUsuarioForm" type="es.oyssen.mrm.struts.forms.usuarios.EditarUsuarioForm" />
		
		
	</form-beans>
	

	<action-mappings>

		<action path="/asignaturas/inicio" forward="/jsp/web/asignaturas/inicio.jsp" />
		
		<action path="/asignaturas/informacion" forward="/jsp/web/asignaturas/informacion.jsp" />
		
		<action path="/asignaturas/estancia" forward="/jsp/web/asignaturas/estancia.jsp" />
		
		<action path="/asignaturas/seminarios" forward="/jsp/web/asignaturas/seminarios.jsp" />
		
		<action path="/asignaturas/trabajos" forward="/jsp/web/asignaturas/trabajos.jsp" />
		
		<action path="/asignaturas/casos" forward="/jsp/web/asignaturas/casos.jsp" />
		
		<action path="/asignaturas/gridusuarios" name="GridUsuarios" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.usuarios.GridUsuariosAction" />
		
		<action path="/asignaturas/editarusuario" name="EditarUsuarioForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.usuarios.EditarUsuarioAction"  />
		
		<action path="/asignaturas/actualizarusuario" name="EditarUsuarioForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.usuarios.EditarUsuarioAction"  />
		
		
	</action-mappings>

</struts-config>