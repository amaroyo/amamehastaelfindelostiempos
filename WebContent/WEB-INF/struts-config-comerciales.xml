<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>	
	
	<form-beans>
		<form-bean name="GridComercialesForm" type="es.oyssen.mrm.struts.forms.comerciales.GridComercialesForm" />		
		<form-bean name="EditarComercialForm" type="es.oyssen.mrm.struts.forms.comerciales.ComercialForm" />
	</form-beans>
	

	<action-mappings>
		<action path="/comerciales/inicio" forward="/jsp/web/comerciales/inicio.jsp" />
		
		<action path="/comerciales/gridcomerciales" name="GridComercialesForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.comerciales.GridComercialesAction">

		</action>


		<action path="/comerciales/gridleads" name="GridComercialesForm" scope="request"  type="es.oyssen.mrm.struts.actions.comerciales.GridLeadsAction">

		</action>

		<action path="/comerciales/editarcomercial" name="EditarComercialForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.comerciales.EditarComercialAction"  />
		
		<action path="/comerciales/actualizarcomercial" name="EditarComercialForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.comerciales.EditarComercialAction"  />
		
		<action path="/comerciales/exportarcomerciales" name="GridComercialesForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.comerciales.ExportarComercialesAction" />
				
	</action-mappings>

</struts-config>