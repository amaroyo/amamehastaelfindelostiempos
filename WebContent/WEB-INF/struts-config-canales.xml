<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>	
	
	<form-beans>
		<form-bean name="GridCanalesForm" type="es.oyssen.mrm.struts.forms.canales.GridCanalesForm" />
		<form-bean name="EditarCanalForm" type="es.oyssen.mrm.struts.forms.canales.CanalForm" />
		
		<form-bean name="GridContactosCanalForm" type="es.oyssen.mrm.struts.forms.contactos.canal.GridContactosCanalForm" />
		<form-bean name="EditarContactoCanalForm" type="es.oyssen.mrm.struts.forms.contactos.canal.ContactoCanalForm" />		
	</form-beans>
	

	<action-mappings>
		<action path="/canales/inicio" forward="/jsp/web/canales/inicio.jsp" />
		
		<action path="/canales/gridcanales" name="GridCanalesForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.canales.GridCanalesAction" />

		<action path="/canales/editarcanal" name="EditarCanalForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.canales.EditarCanalAction"  />
		
		<action path="/canales/actualizarcanal" name="EditarCanalForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.canales.EditarCanalAction"  />
		
		
		<action path="/canales/gridcontactoscanal" name="GridContactosCanalForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.contactos.canal.GridContactosCanalAction" />

		<action path="/canales/editarcontactocanal" name="EditarContactoCanalForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.contactos.canal.EditarContactoCanalAction"  />
		
		<action path="/canales/actualizarcontactocanal" name="EditarContactoCanalForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.contactos.canal.EditarContactoCanalAction"  />
		
		
		<action path="/canales/gridleads" name="GridCanalesForm" scope="request"  type="es.oyssen.mrm.struts.actions.canales.GridLeadsAction" />
		
		<action path="/canales/gridcomerciales" name="GridCanalesForm" scope="request"  type="es.oyssen.mrm.struts.actions.canales.GridComercialesAction" />
		
		<action path="/canales/griddistribuidores" name="GridCanalesForm" scope="request"  type="es.oyssen.mrm.struts.actions.canales.GridDistribuidoresAction" />
		
		<action path="/canales/exportarcanales" name="GridCanalesForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.canales.ExportarCanalesAction" />
		
	</action-mappings>

</struts-config>