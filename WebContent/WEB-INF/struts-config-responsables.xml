<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>	
	
	<form-beans>
		<form-bean name="GridResponsablesForm" type="es.oyssen.mrm.struts.forms.responsables.GridResponsablesForm" />		
		<form-bean name="EditarResponsableForm" type="es.oyssen.mrm.struts.forms.responsables.ResponsableForm" />
	</form-beans>
	

	<action-mappings>
		<action path="/responsables/inicio" forward="/jsp/web/responsables/inicio.jsp" />
		
		<action path="/responsables/gridresponsables" name="GridResponsablesForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.responsables.GridResponsablesAction">

		</action>
				
		<action path="/responsables/gridleads" name="GridResponsablesForm" scope="request"  type="es.oyssen.mrm.struts.actions.responsables.GridLeadsAction">

		</action>

		<action path="/responsables/gridcomerciales" name="GridResponsablesForm" scope="request"  type="es.oyssen.mrm.struts.actions.responsables.GridComercialesAction">

		</action>
				
		<action path="/responsables/editarresponsable" name="EditarResponsableForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.responsables.EditarResponsableAction"  />
		
		<action path="/responsables/actualizarresponsable" name="EditarResponsableForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.responsables.EditarResponsableAction"  />
		
		<action path="/responsables/exportarresponsables" name="GridResponsablesForm" scope="request" validate="false" type="es.oyssen.mrm.struts.actions.responsables.ExportarResponsablesAction" />				
						
	</action-mappings>

</struts-config>